  "license": "MIT",
  "peerDependencies": {
    "@liquid-state/ui-kit": "0.4.0"
  },
  "dependencies": {
    "@liquid-state/ui-kit": "0.4.0",
    "antd": "^3.2.0",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^4.0.8",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.9.0",
